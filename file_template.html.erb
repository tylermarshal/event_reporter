<html>
<head>
  <meta charset='utf-8'>
  <title>Queue Template</title>
</head>
<body>
  <p> Hi </p>
  <table>
  <tr>
    <th>Date</th>
    <th>Last Name</th>
    <th>First Name</th>
    <th>Email</th>
    <th>Zipcode</th>
    <th>City</th>
    <th>State</th>
    <th>Address</th>
    <th>Phone</th></tr>
    <% queue.each do |row| %>
      <tr>
        <td><%= row[:regdate] %></td>
        <td><%= row[:last_name] %></td>
        <td><%= row[:first_name] %></td>
        <td><%= row[:email_address] %></td>
        <td><%= row[:zipcode] %></td>
        <td><%= row[:city] %></td>
        <td><%= row[:state] %></td>
        <td><%= row[:street] %></td>
        <td><%= row[:homephone] %></td>
      </tr>
    <% end %>
  </table>
</body>
</html>
